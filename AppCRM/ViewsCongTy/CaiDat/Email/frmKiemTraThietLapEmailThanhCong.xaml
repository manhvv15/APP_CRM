        xmlns:local="clr-namespace:AppCRM.Views.CaiDat.Email"
        mc:Ignorable="d"
        WindowStyle="None"
        Title="frmKiemTraThietLapEmailThanhCong" Height="90" Width="390">
    <DockPanel>
        <Grid VerticalAlignment="Center">
            <Path Data="{DynamicResource iconThanhCong}" Stroke="Green"></Path>
